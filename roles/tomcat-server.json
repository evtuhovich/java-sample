{
  "name": "tomcat-server",
  "description": "Installs and configures Tomcat server",
  "json_class": "Chef::Role",
  "default_attributes": {
    "java": {
      "jdk_version": "8",
      "install_flavor": "oracle",
      "oracle": {
        "accept_oracle_download_terms": true
      },
      "ark_timeout": 600,
      "ark_download_timeout": 600
    },
    "java-sample": {
      "server": {
        "version": "8.0.32"
      }
    }
  },

  "override_attributes": {
  },
  "chef_type": "role",
  "run_list": [
    "recipe[apt]",
    "recipe[java]",
    "recipe[postgresql_lwrp::apt_official_repository]",
    "recipe[java-sample]",
    "recipe[java-sample::database]"
  ],
  "env_run_lists": {
  }
}
